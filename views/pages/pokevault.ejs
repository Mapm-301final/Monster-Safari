<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head') %>
    </head>
    <body>
        <header>
            <%- include('../partials/header') %>
        </header>
        <main id="main">
                <% pokeVault.forEach(item => { %>
                    <section id="pokeDisplay">
                            <p>Name: <%= item.name %></p>
                            <p>Type: <%= item.type %></p>
                            <p><img src="<%= item.img %>"></p>
                        </section>
                        <% }); %>
                       
        </main>
        <footer>
            <%- include('../partials/footer') %>
        </footer>
        
    </body>
</html>


